{
  "name": "toolset-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typescript": "3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage",
    "coverage:dev": "driveup && react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "24.0.0",
    "@types/node": "10.12.21",
    "@types/react": "16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-logger": "^3.0.6",
    "driveup": "1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "react-testing-library": "^6.0.2",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/*.{ts,tsx}",
      "!**/src/**/index.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "babel": {
    "retainLines": true
  }
}
