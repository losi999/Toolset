{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark": [
            true,
            "single"
        ],
        "comment-format": false,
        "interface-name": false,
        "no-empty": false,
        "radix": false,
        "variable-name": false,
        "max-line-length": [true, 180],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}